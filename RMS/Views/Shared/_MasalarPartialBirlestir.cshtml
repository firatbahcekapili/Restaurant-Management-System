@model List<RMS.Models.Masalar>



@foreach (var item in Model)
{
    if (item.Durum == "Dolu")
    {
        <a id="btn" class="btn col-3 small w-25 text-responsive m-1 d-flex justify-content-center modallink  pt-2 pb-2 modalMasa-@item.MasaId @item.Konum" onclick="Tikla(this,@item.MasaId)" style="background-color: #008cff;">

            <div class="col d-flex justify-content-center align-items-center" style="font-size:13px"><i class="fas fa-chair   text-gray-300 ">  @item.MasaAd</i> </div>

        </a>
    }




}


